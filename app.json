{
  "expo": {
    "name": "tocconstruction",
    "slug": "tocconstruction",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.davidadeolu.tocconstruction",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "plugins": ["expo-notifications"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidadeolu.tocconstruction"
    },
    "android": {
      "package": "com.davidadeolu.tocconstruction",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
